{
	"compilerOptions": {
		"module": "commonjs",
		"esModuleInterop": true,
		"outDir": "dist",
		"rootDir": "src",
		"target": "es6",
		"strict": true,
		"resolveJsonModule": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": ["src/**/*", "prisma/seed.ts"],
	"exclude": ["node_modules"]
}
